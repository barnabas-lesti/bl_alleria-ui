{
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"env": {
		"es6": true,
		"node": true,
		"browser": true
	},
	"rules": {
		"indent": [
			2,
			"tab"
		],
		"quotes": [
			2,
			"single"
		],
		"semi": 2,
		"no-console": 0,
		"no-undef": 2
	}
}
